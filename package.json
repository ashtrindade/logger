{
  "name": "@ashtrindade/logger",
  "version": "1.0.5",
  "description": "Node.js logger with dynamic log levels",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest --verbose ./test",
    "test:coverage": "jest --verbose ./test/ --coverage",
    "build": "tsc",
    "lint": "npx eslint . --fix",
    "reset": "rm -rf node_modules && rm package-lock.json && npm install",
    "clear:build": "rm -rf dist",
    "clear:coverage": "rm -rf coverage",
    "prepare": "husky install",
    "help": "bash scripts/scripts-list.sh",
    "pkg:publish": "rm -rf dist && npm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashtrindade/logger.git"
  },
  "author": "Ash Trindade",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashtrindade/logger/issues"
  },
  "homepage": "https://github.com/ashtrindade/logger#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "directories": {
    "test": "test"
  }
}
